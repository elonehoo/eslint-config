{
  "name": "@elonehoo/eslint-config-monorepo",
  "license": "MIT",
  "version": "1.0.2",
  "author": "Elone Hoo <huchengyea@163.com> (https://github.com/elonehoo)",
  "description": "Elone Hoo's ESLint config presets",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "core:publish": "pnpm -C packages/core run publish",
    "basic:publish": "pnpm -C packages/basic run publish",
    "ts:publish": "pnpm -C packages/typescript run publish",
    "vue:publish": "pnpm -C packages/vue run publish",
    "react:publish": "pnpm -C packages/react run publish",
    "all:publish": "pnpm -C packages/all run publish"
  },
  "keywords": [
    "esline",
    "config"
  ],
  "devDependencies": {
    "@elonehoo/eslint-config": "*",
    "bumpp": "^7.1.1",
    "eslint": "^8.14.0",
    "eslint-plugin-elonehoo": "workspace:*",
    "typescript": "^4.6.3"
  }
}
