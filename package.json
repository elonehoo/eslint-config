{
  "name": "@elonehoo/eslint-config-monorepo",
  "license": "MIT",
  "version": "1.0.0",
  "author": "Elone Hoo <huchengyea@163.com> (https://github.com/elonehoo)",
  "description": "Elone Hoo's ESLint config presets",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "publish": "pnpm -C packages/eslint-plugin-elonehoo run publish --access public && pnpm -C packages/"
  },
  "keywords": [
    "esline",
    "config"
  ],
  "devDependencies": {
    "@elonehoo/eslint-config": "*",
    "bumpp": "^7.1.1",
    "eslint": "^8.14.0",
    "eslint-plugin-elonehoo": "workspace:*",
    "typescript": "^4.6.3"
  }
}
